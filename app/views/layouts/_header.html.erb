<div id="header">
  <h1 id="logo"><%= link_to "#" %></h1>
  <div class="social">
  <% if user_signed_in? %>
    <ul>
      <li><%= link_to t(".logout"), destroy_user_session_path,  method: :delete %></li>
    </ul>
  <% else %>
    <ul>
      <li><%= link_to t(".sign_up"), new_user_registration_path %></li>
      <li><%= link_to t(".log_in"), new_user_session_path %></li>
    </ul>
  <% end %>
  </div>
  <div id="navigation">
    <ul>
      <li class="float-right">
        <section class="search">
          <%= form_tag("#", method: :get, class: "searchbar") do |f| %>
            <%= text_field_tag :search, "", class: "search_input", placeholder: t(".search_placeholder") %>
          <% end %>
        </section>
      </li>
    </ul>
  </div>
</div>
